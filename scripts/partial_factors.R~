model_formula <- as.formula(paste("ln_sale_price ~ after_flood*treatment+ ",model_all))

data <- results[["F_NFP"]][["model_data"]][["matched_data"]]

fit <- lm(model_formula, data = data)

eff <- effect("after_flood:treatment",fit)

eff_frame <- as.data.frame(eff)

ggplot(eff_frame, aes(after_flood, fit, color=treatment)) + geom_point() + geom_errorbar(aes(ymin=fit-se, ymax=fit+se), width=0.4) + theme_bw(base_size=12)